    <nav dir="rtl" id="nav_bar" class="flex text-white justify-around  items-center">
      <div id="h1_text" class="flex justify-between ">
        <h1 class="text-3xl">المطور عبد الرحمن</h1>
        <i id="menu" class="fa-solid fa-bars" style="color: #ffffff; font-size: 35px;"></i>

      </div>

      <ul id="menus" class="flex mx-28 text-xl  ">
